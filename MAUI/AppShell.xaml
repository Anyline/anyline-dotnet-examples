<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="Anyline.Examples.MAUI.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:Anyline.Examples.MAUI"
    FlyoutBehavior="Disabled"
    NavigationPage.HasBackButton="True"
    >


    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />

    <ShellContent
        Title="Scanning Page"
        ContentTemplate="{DataTemplate local:MyScanningWithAnylinePage}"
        Route="MyScanningWithAnylinePage" />

</Shell>
